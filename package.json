{
  "name": "303-lite",
  "version": "1.0.0",
  "description": "Modern bassline synthesizer inspired by the Roland TB-303",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run watch\" \"npm run electron-dev\"",
    "watch": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "electron-dev": "wait-on dist/index.html && electron ."
  },
  "keywords": ["synthesizer", "bassline", "303", "music", "audio"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "electron": "^27.0.0",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
